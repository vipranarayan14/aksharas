{
  "compilerOptions": {
    "target": "ES2016",
    "module": "commonjs",
    "sourceMap": true,
    "strict": true,
    "isolatedModules": true,
    "noEmit": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"],
  "$schema": "https://json.schemastore.org/tsconfig"
}
